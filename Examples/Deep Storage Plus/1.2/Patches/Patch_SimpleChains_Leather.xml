<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Simple Chains: Leather</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="DSP_Bin"]/building/fixedStorageSettings/filter/categories</xpath>
          <value>
            <li>Hides</li>
            <li>PreparedHides</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName="DSP_Barrel"]/building/fixedStorageSettings/filter/thingDefs</xpath>
          <value>
            <li>Owl_TanningLiquor</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>