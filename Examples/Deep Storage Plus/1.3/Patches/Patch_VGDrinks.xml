<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>VGP Garden Drinks</li>
      <li>VGP Garden Drinks UF</li>
    </mods>
    <!-- <success>Always</success> -->
    <!-- Add Vegetable Garden drinks to Barrel -->
    <match Class="PatchOperationAdd">
      <xpath>/Defs/ThingDef[defName="DSP_Barrel"]/building/fixedStorageSettings/filter/categories</xpath>
      <value>
        <li>VG_Drinks</li>
        <li>VG_Sodas</li>
      </value>
    </match>
  </Operation>
</Patch>