	<ThingDef ParentName="TE_BuildingBase">
		<defName>PitfallTrap</defName>
		<label>pitfall trap</label>
		<thingClass>Building_TrapDamager</thingClass>
		<graphicData>
			<texPath>Things/Building/SpikePit/SpikePitArmed</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<rect>(0,0,1,0.15)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<drawerType>RealtimeOnly</drawerType>
		<rotatable>false</rotatable>
		<tickerType>Normal</tickerType>
		<stealable>false</stealable>		
		<statBases>
			<Mass>2.5</Mass>
			<MaxHitPoints>60</MaxHitPoints>
			<WorkToBuild>4000</WorkToBuild>
			<Beauty>-10</Beauty>
			<TrapMeleeDamage>70</TrapMeleeDamage>
			<TrapSpringChance>0.8</TrapSpringChance>
		</statBases>
		<description>A pit dug in the ground full of spikes with fake ground cover. When the victim steps on the fake cover, the victim falls into the pit and is impaled by spikes.</description>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>40</costStuffCount>
		<designationCategory>Security</designationCategory>
		<building>
			<isTrap>true</isTrap>
			<trapDamageCategory>Sharp</trapDamageCategory>
			<trapDestroyOnSpring>true</trapDestroyOnSpring>
			<ai_chillDestination>false</ai_chillDestination>
			<expandHomeArea>false</expandHomeArea>
			<trapUnarmedGraphicData>
				<texPath>Things/Building/SpikePit/SpikePitUnarmed</texPath>
				<graphicClass>Graphic_Single</graphicClass>
				<shaderType>Cutout</shaderType>
			</trapUnarmedGraphicData>
		</building>
		<constructionSkillPrerequisite>3</constructionSkillPrerequisite>
		<terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
		<researchPrerequisites>
			<li>BasicTraps</li>
		</researchPrerequisites>
		<placeWorkers>
      <li>PlaceWorker_NeverAdjacentTrap</li>
    </placeWorkers>
	</ThingDef>

	<ThingDef ParentName="TE_FurnitureBase">
		<defName>StorageBox</defName>
		<label>storage box</label>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
			<texPath>Things/Building/Furniture/StorageBox</texPath>
