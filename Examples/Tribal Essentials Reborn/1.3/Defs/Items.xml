<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="TE_ResourceBase" Abstract="True">
		<thingClass>ThingWithComps</thingClass>
		<category>Item</category>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
		<stackLimit>75</stackLimit>
		<statBases>
			<Beauty>-7</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
	</ThingDef>

	<ThingDef Abstract="True" Name="TE_OrganicProductBase" ParentName="TE_ResourceBase">
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>10</DeteriorationRate>
			<Mass>0.03</Mass>
		</statBases>
		<tickerType>Rare</tickerType>
	</ThingDef>

	<ThingDef ParentName="TE_OrganicProductBase">
		<defName>CookedEgg</defName>
		<label>cooked egg</label>
		<thingClass>ThingWithComps</thingClass>
		<thingCategories>
			<li>AnimalProductRaw</li>
		</thingCategories>
		<stackLimit>75</stackLimit>
		<description>A cooked egg. Tastes better than raw.</description>
		<graphicData>
			<texPath>Things/Item/CookedEgg</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(245,245,245)</color>
		</graphicData>
		<statBases>
			<MarketValue>7.0</MarketValue>
			<Mass>0.15</Mass>
			<WorkToMake>500</WorkToMake>
			<Nutrition>0.25</Nutrition>
		</statBases>
		<ingestible>
			<foodType>Meal</foodType>
			<preferability>MealSimple</preferability>

			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<tasteThought/> 
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>7</daysToRotStart>
				<rotDestroys>true</rotDestroys> 
			</li>
			<li Class="CompProperties_Ingredients" />
			<li Class="CompProperties_FoodPoisonable" />
		</comps>
	</ThingDef>

	<ThingDef ParentName="TE_OrganicProductBase">
		<defName>Popcorn</defName>
		<label>popcorn</label>
		<thingClass>ThingWithComps</thingClass>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<stackLimit>75</stackLimit>
		<description>Tasty, tasty popcorn!</description>
		<graphicData>
			<texPath>Things/Item/Popcorn/Popcorn</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>6.0</MarketValue>
			<Mass>0.025</Mass>
			<WorkToMake>500</WorkToMake>
			<Nutrition>0.05</Nutrition>
		</statBases>
		<ingestible>
			<foodType>Meal</foodType>
			<preferability>MealFine</preferability>

			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<joy>0.10</joy>
			<joyKind>Gluttonous</joyKind>
			<maxNumToIngestAtOnce>10</maxNumToIngestAtOnce>
			<tasteThought/> 
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>7</daysToRotStart>
				<rotDestroys>true</rotDestroys> 
			</li>
			<li Class="CompProperties_Ingredients" />
			<li Class="CompProperties_FoodPoisonable" />
		</comps>
	</ThingDef>	

	<ThingDef Name="TE_UnfinishedBase" Abstract="True">
		<thingClass>UnfinishedThing</thingClass>
		<category>Item</category>
		<label>unfinished thing</label>
		<graphicData>
			<texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<useHitPoints>true</useHitPoints>
		<isUnfinishedThing>true</isUnfinishedThing>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<selectable>true</selectable>
		<tradeability>None</tradeability>
		<drawerType>RealtimeOnly</drawerType>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<DeteriorationRate>2.0</DeteriorationRate>
			<Beauty>-6</Beauty>
			<Mass>1</Mass>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
		<thingCategories>
			<li>Unfinished</li>
		</thingCategories>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
	</ThingDef>

	<ThingDef ParentName="TE_UnfinishedBase">
		<defName>UnfinishedRug</defName>
		<label>unfinished rug</label>
		<graphicData>
			<texPath>Things/Item/Unfinished/UnfinishedApparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
			<li>Metallic</li>
		</stuffCategories>
		<comps>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
	</ThingDef>

</Defs>
