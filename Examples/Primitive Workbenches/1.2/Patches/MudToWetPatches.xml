<Patch>

    
	<!--  =========================  Goblins  ========================  -->
	
	
	<Operation Class="PatchOperationFindMod">
        <success>Always</success>
        <mods>
          <li>Fantasy Goblins Updated</li>
        </mods>
        <match Class="PatchOperationSequence">			
            <operations>
                <li Class="PatchOperationAdd">					
		          <xpath>Defs</xpath>
		            <value>
					  <RecipeDef>
						<defName>VBY_MakeClayFromGoblins</defName>
						<label>make wet clay clumps</label>
						<description>Make wet clay clumps from mud and haygrass.\n\nMakes 20 clumps of wet clay.</description>
						<jobString>Making wet clay clumps.</jobString>
						<effectWorking>Sculpt</effectWorking>
						<soundWorking>Recipe_Sculpt</soundWorking>
						<workAmount>200</workAmount>
						<ingredients>
							<li>
								<filter>
									<thingDefs>
										<li>GoblinMud</li>
									</thingDefs>
								</filter>
								<count>100</count>
							</li>
							<li>
								<filter>
									<thingDefs>
										<li>Hay</li>
									</thingDefs>
								</filter>
								<count>50</count>
							</li>
						</ingredients>
						<fixedIngredientFilter>
							<thingDefs>
								<li>GoblinMud</li>
								<li>Hay</li>
							</thingDefs>
						</fixedIngredientFilter>
						<products>
							<VBY_ClayPaste>20</VBY_ClayPaste>
						</products>
						<!--<workSkill>Crafting</workSkill>-->
						<recipeUsers>
							<li>CraftingSpot</li>
							<li>VBY_PrimitiveCraftingTable</li>
							<li>VBY_PrimitiveKiln</li>
							<li>VBY_PrimitiveKiln_Rotated</li>
						</recipeUsers>
					  </RecipeDef>
		            </value>
				</li>
			</operations>
        </match>
	</Operation>
	
