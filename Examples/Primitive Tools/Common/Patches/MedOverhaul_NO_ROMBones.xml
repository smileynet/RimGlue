<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Medieval Overhaul</li>
    </mods>
    <match Class="PatchOperationFindMod">
    <mods>
      <li>Rim of Madness - Bones</li>
    </mods>
		<match Class="PatchOperationSequence">
			<operations>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneNeedleMO"]</xpath>
			  </li>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneShovelMO"]</xpath>
			  </li>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneKnifeMO"]</xpath>
			  </li>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneWhistleMO"]</xpath>
			  </li>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneHarpoonMO"]</xpath>
			  </li>
			  <li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "VBY_BoneSawMO"]</xpath>
			  </li>		
			</operations>
		</match>
	</match>
  </Operation>

</Patch>