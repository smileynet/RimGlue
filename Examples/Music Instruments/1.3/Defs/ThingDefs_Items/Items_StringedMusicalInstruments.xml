<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="StringInstrumentBase">
    <label>guitar</label>
    <defName>Guitar</defName>
    <description>A six-stringed instrument played by picking or strumming. Great for entertaining groups around a campfire.</description>
    <graphicData>
      <texPath>Things/Items/String/Guitar</texPath>
    </graphicData>
    <techLevel>Medieval</techLevel>
    <statBases>
      <WorkToMake>15000</WorkToMake>
      <Mass>6</Mass>
    </statBases>
    <costStuffCount>100</costStuffCount>
    <comps>
      <li Class="MusicalInstruments.CompProperties_MusicalInstrument">
        <compClass>MusicalInstruments.CompMusicalInstrument</compClass>
        <easiness>0.5</easiness>
        <expressiveness>0.5</expressiveness>
        <xOffset>0.2</xOffset>
        <zOffset>-0.2</zOffset>
        <xOffsetFacing>0.0</xOffsetFacing>
        <zOffsetFacing>-0.2</zOffsetFacing>
      </li>
    </comps>
  </ThingDef>
  <ThingDef ParentName="StringInstrumentBase">
    <label>bass guitar</label>
    <defName>BassGuitar</defName>
    <description>A guitar that has the same pitch and tuning as a double bass, usually electrically amplified</description>
    <graphicData>
      <texPath>Things/Items/String/BassGuitar</texPath>
    </graphicData>
    <techLevel>Industrial</techLevel>
    <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
    <statBases>
      <WorkToMake>20000</WorkToMake>
      <Mass>8</Mass>
    </statBases>
    <costStuffCount>100</costStuffCount>
    <costList>
      <Steel>5</Steel>
    </costList>
    <comps>
      <li Class="MusicalInstruments.CompProperties_MusicalInstrument">
        <compClass>MusicalInstruments.CompMusicalInstrument</compClass>
        <easiness>0.6</easiness>
        <expressiveness>0.5</expressiveness>
        <xOffset>0.2</xOffset>
        <zOffset>-0.2</zOffset>
        <xOffsetFacing>0.0</xOffsetFacing>
        <zOffsetFacing>-0.2</zOffsetFacing>
      </li>
    </comps>
  </ThingDef>
